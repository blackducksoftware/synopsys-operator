<button onclick="sendBlackDuck()">Send Black Duck</button>
<script>
    function sendBlackDuck() {
        alert("Sending Black Duck Data")
        var bdPatches = {
            patch: "patch_parameter"
        }
        var xhr = new XMLHttpRequest();
        xhr.open('POST', "http://localhost:8081", true);
        xhr.setRequestHeader("Content-Type", "application/json");
        xhr.send(JSON.stringify(bdPatches));
        alert("Sent Black Duck Data")
    } 
</script>
